{
  "name": "@leancloud/play",
  "version": "0.18.0-beta.13",
  "protocolVersion": "0",
  "description": "LeanCloud Play SDK",
  "main": "dist/play-node.js",
  "scripts": {
    "precommit": "pretty-quick --staged",
    "protoc": "protoc --js_out=import_style=commonjs_strict,binary:./src/ ./proto/*.proto",
    "lint": "eslint src test && tsc play.d.ts",
    "build": "rollup -c",
    "test:node": "nyc mocha test/**.js --exit",
    "test": "npm run lint && npm run test:node",
    "doc": "jsdoc src README.md package.json -c .jsdocrc.json -d ./doc/"
  },
  "license": "MIT",
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "debug": "^3.1.0",
    "eventemitter3": "^3.1.0",
    "google-protobuf": "^3.10.0",
    "isomorphic-ws": "^4.0.1",
    "javascript-state-machine": "^3.1.0",
    "machina": "^4.0.2",
    "query-string": "^6.9.0",
    "superagent": "^3.8.3",
    "url-join": "^4.0.1",
    "ws": "^5.2.1"
  },
  "devDependencies": {
    "@leeyeh/jsdoc-rtd": "^1.0.14",
    "@types/node": "~10.9",
    "babel-cli": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "husky": "^0.14.3",
    "jsdoc": "^3.5.5",
    "jsdoc-export-default-interop": "^0.3.1",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "prettier": "^1.13.5",
    "pretty-quick": "^1.6.0",
    "rollup": "^0.61.2",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-babel-minify": "^6.2.0",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-modify": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "typescript": "^2.9.2",
    "weapp-polyfill": "^3.2.1"
  },
  "types": "./play.d.ts",
  "browser": "dist/play.js",
  "weapp": "dist/play-weapp.js",
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "include": [
      "src/**"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "publishConfig": {
    "access": "public"
  }
}
