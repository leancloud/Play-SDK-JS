{
  "extends": ["airbnb-base", "prettier", "plugin:prettier/recommended"],
  "env": {
    "node": true,
    "mocha": true
  },
  "rules": {
    "prettier/prettier": 2,
    "no-console": [2, { "allow": ["warn", "error"] }],
    "no-underscore-dangle": 0,
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }]
  }
}
